        <!-- The Modal -->
        <div id="myModal" class="modal">

          <!-- Modal content -->
          <div class="modal-content">
            <div class="modal-header">
              <span class="close">&times;</span>
              <h2>Modal Header</h2>
            </div>
            <div class="modal-body">
                <figure id="" class="">
                    <img src="imgFilmes/goosebumpsDestaque.jpg">
                </figure>
            </div>
            <div class="modal-footer">
